<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:orientation="vertical"

    tools:context="com.example.administrator.huanxing.fragment.FriendsFragment">



<com.hyphenate.easeui.widget.EaseTitleBar
    android:id="@+id/title_bar"
    android:layout_width="match_parent"
    app:titleBarLeftImage="@drawable/ease_mm_title_back"
    android:layout_height="50dp"></com.hyphenate.easeui.widget.EaseTitleBar>

    <com.hyphenate.easeui.widget.EaseContactList
        android:id="@+id/friends_listview"
        ctsListInitialLetterBg="#abc"
        android:layout_width="match_parent"
        android:layout_height="0dp"
        android:layout_weight="1"></com.hyphenate.easeui.widget.EaseContactList>
</LinearLayout>
